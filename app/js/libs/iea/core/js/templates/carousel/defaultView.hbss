<ul class="iea-carousel clearfix">
    {{#each carousel.slides}}
        <li class="{{#if panel.image}} image {{else}} video {{/if}}">
            {{#if panel.image}}
                {{include this "panel/imagePanel.hbss" }}
            {{else}}
                {{include this "panel/videoPanel.hbss" }}
            {{/if}}
        </li>
    {{/each}}
</ul>
