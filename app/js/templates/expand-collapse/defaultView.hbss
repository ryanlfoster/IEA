{{#each expandCollapse.panels}}
<div class="expandcollapse-panel">
	<div class="expandcollapse-header {{#if expanded}} expanded {{else}} collapsed {{/if}}">
	    <a class="expandcollapse-toggle" href="#">
	        <h3>{{heading}}</h3>
	    </a>
	</div>
	<div class="expandcollapse-body {{#if expanded}} {{else}} hiding {{/if}}">
	    <img src="{{imagePath}}"/>
	    <p>{{{content}}}</p>
	</div>
</div>
{{/each}}